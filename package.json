  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node -O '{ \"module\": \"commonjs\" }' build.ts",
    "build-data": "ts-node -O '{ \"module\": \"commonjs\" }' scripts/build-data.ts",
